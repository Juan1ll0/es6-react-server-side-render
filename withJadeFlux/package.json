{
  "name": "react-server-side-render",
  "version": "0.0.1",
  "description": "An example to react render in server",
  "main": "app.js",
  "scripts": {
    "start-dev-backend-server": "nodemon --exec babel-node --presets es2015,react --ignore src/ server.js",
    "start-dev-frontend-server": "webpack --watch",
    "start-dev-server": "npm run start-dev-backend-server & npm run start-dev-frontend-server"
  },
  "keywords": [
    "react",
    "node",
    "server-render"
  ],
  "author": "Juan Ignacio Fernandez Diez",
  "license": "MIT",
  "dependencies": {
    "express": "^4.13.4",
    "flux": "^2.1.1",
    "jade": "^1.11.0",
    "react": "^0.14.8",
    "react-dom": "^0.14.8",
    "serve-favicon": "^2.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "nodemon": "^1.9.1",
    "webpack": "^1.12.14"
  }
}
